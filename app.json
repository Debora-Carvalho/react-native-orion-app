{
  "expo": {
    "name": "react-native-orion-app",
    "slug": "react-native-orion-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-orion-branca.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-orion-branca.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0c2a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-orion-branca.png",
        "backgroundColor": "#0b0c2a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
